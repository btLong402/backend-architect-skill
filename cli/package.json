{
    "name": "backend-architect-cli",
    "version": "2.2.0",
    "description": "CLI to install Backend Architect skill for AI coding assistants",
    "type": "module",
    "bin": {
        "backend-architect": "./dist/index.js"
    },
    "files": [
        "dist",
        "assets",
        "LICENSE"
    ],
    "scripts": {
        "sync:assets": "bash scripts/sync-assets.sh",
        "build": "npm run sync:assets && tsc",
        "build:bun": "npm run sync:assets && bun build src/index.ts --outdir dist --target node",
        "dev": "npx ts-node --esm src/index.ts",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "backend",
        "architecture",
        "microservices",
        "database",
        "security",
        "claude",
        "cursor",
        "windsurf",
        "copilot",
        "kiro",
        "roocode",
        "codex",
        "qoder",
        "gemini",
        "trae",
        "opencode",
        "continue",
        "codebuddy",
        "antigravity",
        "ai",
        "skill"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "commander": "^12.1.0",
        "chalk": "^5.3.0",
        "ora": "^8.1.1",
        "prompts": "^2.4.2"
    },
    "devDependencies": {
        "@types/bun": "^1.1.14",
        "@types/node": "^22.10.1",
        "@types/prompts": "^2.4.9",
        "typescript": "^5.7.2"
    }
}
